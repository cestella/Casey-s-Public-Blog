---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Structure &amp; Process</title>
 <link href="http://www.caseystella.com/blog/atom.xml" rel="self"/>
 <link href="http://www.caseystella.com/blog/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.caseystella.com/blog/</id>
 <author>
   <name>Casey Stella</name>
 </author>
 
 {% for post in site.categories.blog limit:5 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.caseystella.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>id:{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
